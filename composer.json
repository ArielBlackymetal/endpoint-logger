{
    "name": "arielblackymetal/endpoint-logger",
    "description": "Un driver de logging de Laravel para enviar logs a un endpoint.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "ArielBlackymetal\\EndpointLogger\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "illuminate/log": "^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArielBlackymetal\\EndpointLogger\\EndpointLoggerServiceProvider"
            ]
        }
    }
}
